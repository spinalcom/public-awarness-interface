{
  "name": "spinal-browser-pai",
  "description": "public awareness interface",
  "version": "0.1.0",
  "scripts": {
    "watch": "parcel watch src/index.html --out-dir build --public-url /html/pai ",
    "serve": "parcel src/index.html --out-dir build --public-url /html/pai --no-minify  --no-cache",
    "build": "parcel build src/index.html --out-dir build --public-url /html/pai --no-minify",
    "postinstall": "npm run spinal-register && npm run build",
    "spinal-register": "spinal-register --browser 'spinal-browser-'"
  },
  "dependencies": {
    "csv-stringify": "^5.3.0",
    "file-saver": "^2.0.2",
    "horizontal-date-picker": "git+https://github.com/LorisC/horizontal-date-picker.git",
    "import": "0.0.6",
    "moment": "^2.24.0",
    "spinal-core-connectorjs_type": "git+https://github.com/spinalcom/spinal-core-connectorjs_type.git",
    "spinal-env-viewer-graph-service": "git+https://github.com/spinalcom/Spinal-Graph-Service.git",
    "spinal-core-system": "git+https://github.com/spinalcom/spinal-core-system.git",
    "spinal-forge-viewer-vue": "git+https://github.com/spinalcom/spinal-forge-viewer-vue.git",
    "vue": "^2.6.10",
    "vue-cookies": "^1.5.13",
    "vue-router": "^3.0.3",
    "vue-svg-loader": "^0.12.0",
    "vuetify": "^2.0.0",
    "zipcelx": "^1.6.2"
  },
  "devDependencies": {
    "@mdi/font": "^4.2.95",
    "@vue/component-compiler-utils": "^3.0.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  }
}
